import java.io.*;
import java.util.*;
public class FactorialOfNumber {
    public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter n value between 1to 20");
        int n = sc.nextInt();
        int fact=1;
        for(int i=2;i<=n;i++)
        {
            fact = fact*i;
        }
    System.out.println("fact of first " +n+ " numbers is " +fact);
    }
}
